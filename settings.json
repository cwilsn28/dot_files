{
    "editor.fontFamily": "Hack Nerd Font Mono",
    "editor.rulers": [
        80
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "editor.lineNumbers": "relative",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "code-runner.enableAppInsights": false,
    "update.channel": "none",
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.cursorBlinking": "solid",
    "gitlens.views.repositories.showBranchComparison": null,
    "editor.codeActionsOnSave": null,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
        "before": ["j", "j"],
        "after": ["<Esc>"]
        }
    ],
    "vim.leader": ",",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "vim.normalModeKeyBindings": [
        {
            "before": [":"],
            "commands": [
                "workbench.action.showCommands",
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
          "before": ["<C-j>"],
          "after": ["<leader>", "<leader>", "j"]
        },
        {
          "before": ["<C-k>"],
          "after": ["<leader>", "<leader>", "k"]
        }
    ],
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "workbench.preferredDarkColorTheme": "Abyss",
    "python.languageServer": "Pylance",
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": false,
    "workbench.iconTheme": "file-icons-colourless",
    "editor.fontLigatures": null,
    "workbench.colorTheme": "Cobalt2",
    "workbench.preferredHighContrastColorTheme": "SubZero-2.0",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "python.analysis.extraPaths": [
        "./app/esp/core/alpha",
        "/home/vagrant/Lab7_ESP/alpha"
    ],

    "python.autoComplete.extraPaths": [
        "./app/esp/core/alpha"
    ],
    "python.pythonPath": "/usr/local/opt/python@3.7/Frameworks/Python.framework/Versions/3.7/lib/python3.7",
    "yaml.schemas": {
        "file:///Users/christianwilson/.vscode/extensions/atlassian.atlascode-2.10.11/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "workbench.sideBar.location": "right",
    "redhat.telemetry.enabled": false,
    "editor.accessibilitySupport": "off"
}